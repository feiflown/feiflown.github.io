  &lt;script src=&#34;http://feiflown.github.io/js/ua-parser.min.js&#34;&gt;&lt;/script&gt;
  &lt;script&gt;

    $.ajax({
      type: &#34;GET&#34;,
      url: &#34;https://ifconfig.me/ip&#34;,
      success: function (data) {
        console.log(&#34;Your IPv4 is: &#34; + data)
        var ipv4 = data
        $.getJSON(&#34;https://ipapi.co/&#34; + ipv4 + &#34;/json&#34;, function (data) {
