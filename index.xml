  &lt;script type=&#34;text/javascript&#34;&gt;
    console.clear();
    console.log(&#34;东拼西凑的代码，像坨屎一样，我自己都看不下去了.&#34;);
    $(document).ready(function () {
      
      
      
      $(&#39;.hover&#39;).hover(function () {
        $(this).addClass(&#39;flip&#39;);
      }, function () {
        $(this).removeClass(&#39;flip&#39;);
